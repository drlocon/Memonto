<div class="container mt-5">
  <div class="row">
    <div class="col-6 offset-3 text-center">
      <h2 class="font-weight-bold">記録の一覧</h2>
     </div>

    <div class="col-10 offset-1 text-center">
      タグリスト
      <% @tags.each do |list| %>
        <%=link_to list.name, tag_search_documents_path(tag_id: list.id) %>
      <% end %>
    </div>
<div class="col-10 offset-1 text-left">
    <%= render "public/documents/document_card", documents: @documents %>
    </div>
  </div>
</div>
